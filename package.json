{
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/UserOfficeProject/user-office-factory.git"
  },
  "private": true,
  "author": "SWAP",
  "license": "ISC",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll --runInBand --detectOpenHandles",
    "build": "rm -rf ./build && tsc",
    "prod": "npm run build && env NODE_ENV=production node ./build/index.js",
    "dev": "ts-node-dev --rs --trace-warnings --respawn ./src/index.ts",
    "dev:docker": "npm install && ts-node-dev --inspect=0.0.0.0:9229 --rs --respawn ./src/index.ts",
    "debug": "ts-node-dev --rs --trace-warnings --inspect --respawn ./src/index.ts",
    "lint": "tsc --noEmit && eslint ./src --ext .js,.ts --quiet",
    "lint:all": "tsc --noEmit && eslint ./src --ext .js,.ts",
    "lint:fix": "tsc --noEmit && eslint ./src --ext .js,.ts --fix --quiet"
  },
  "dependencies": {
    "@esss-swap/duo-logger": "^1.1.1",
    "await-to-js": "^2.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "gm": "^1.23.1",
    "handlebars": "^4.7.7",
    "http-errors": "~1.8.0",
    "jsbarcode": "^3.11.5",
    "knex": "^0.21.17",
    "morgan": "~1.10.0",
    "muhammara": "^1.10.0",
    "pg": "^8.7.1",
    "pg-large-object": "^2.0.0",
    "puppeteer": "^5.4.1",
    "xlsx": "^0.17.3",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/gm": "^1.18.10",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "@types/node": "^12.20.1",
    "@types/pg": "^7.14.10",
    "@types/pg-large-object": "^2.0.2",
    "@types/puppeteer": "^3.0.5",
    "@types/supertest": "^2.0.10",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.20",
    "prettier": "^1.19.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^3.9.9"
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
