{
  "name": "user-office-factory",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/UserOfficeProject/user-office-factory.git"
  },
  "private": true,
  "author": "SWAP",
  "license": "ISC",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll --runInBand --detectOpenHandles",
    "build": "rm -rf ./build && tsc",
    "prod": "npm run build && env NODE_ENV=production node ./build/index.js",
    "dev": "ts-node-dev --rs --trace-warnings --respawn --exit-child ./src/index.ts",
    "dev:docker": "npm install && ts-node-dev --inspect=0.0.0.0:9229 --rs --respawn ./src/index.ts",
    "debug": "ts-node-dev --rs --trace-warnings --inspect --respawn ./src/index.ts",
    "lint": "tsc --noEmit && eslint ./src --ext .js,.ts --quiet",
    "lint:all": "tsc --noEmit && eslint ./src --ext .js,.ts",
    "lint:fix": "tsc --noEmit && eslint ./src --ext .js,.ts --fix --quiet"
  },
  "dependencies": {
    "@user-office-software/duo-logger": "^2.1.1",
    "await-to-js": "^3.0.0",
    "canvas": "^2.11.2",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gm": "^1.25.0",
    "handlebars": "^4.7.7",
    "http-errors": "^2.0.0",
    "jsbarcode": "^3.11.5",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "muhammara": "^4.0.0",
    "pg": "^8.11.1",
    "pg-large-object": "^2.0.0",
    "puppeteer": "^20.9.0",
    "qrcode": "^1.5.3",
    "xlsx": "^0.18.5",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/gm": "^1.25.1",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@types/pg": "^8.10.2",
    "@types/pg-large-object": "^2.0.2",
    "@types/qrcode": "^1.5.1",
    "@types/supertest": "^2.0.12",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "lodash": "^4.17.20",
    "prettier": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.0.0"
  }
}
