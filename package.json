{
  "name": "user-office-factory",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/UserOfficeProject/user-office-factory.git"
  },
  "private": true,
  "author": "SWAP",
  "license": "ISC",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll --runInBand --detectOpenHandles",
    "build": "rm -rf ./build && tsc",
    "prod": "npm run build && env NODE_ENV=production node ./build/index.js",
    "dev": "ts-node-dev --rs --trace-warnings --respawn ./src/index.ts",
    "dev:docker": "npm install && ts-node-dev --inspect=0.0.0.0:9229 --rs --respawn ./src/index.ts",
    "debug": "ts-node-dev --rs --trace-warnings --inspect --respawn ./src/index.ts",
    "lint": "tsc --noEmit && eslint ./src --ext .js,.ts --quiet",
    "lint:all": "tsc --noEmit && eslint ./src --ext .js,.ts",
    "lint:fix": "tsc --noEmit && eslint ./src --ext .js,.ts --fix --quiet"
  },
  "dependencies": {
    "@user-office-software/duo-logger": "^1.2.0",
    "await-to-js": "^3.0.0",
    "canvas": "^2.8.0",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.3",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "gm": "^1.23.1",
    "handlebars": "^4.7.7",
    "http-errors": "^2.0.0",
    "jsbarcode": "^3.11.5",
    "knex": "^1.0.2",
    "morgan": "^1.10.0",
    "muhammara": "^2.1.0",
    "pg": "^8.7.3",
    "pg-large-object": "^2.0.0",
    "puppeteer": "^13.3.0",
    "qrcode": "^1.5.0",
    "xlsx": "^0.18.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/gm": "^1.18.11",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.0.0",
    "@types/pg": "^8.6.4",
    "@types/pg-large-object": "^2.0.2",
    "@types/qrcode": "^1.4.2",
    "@types/supertest": "^2.0.10",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^4.3.8",
    "jest": "^27.4.7",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.20",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
